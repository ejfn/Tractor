{
  "name": "tractor",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "node scripts/inject-dev-version.js && expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "node scripts/inject-dev-version.js && expo start --android",
    "ios": "node scripts/inject-dev-version.js && expo start --ios",
    "lint": "expo lint",
    "test": "jest",
    "test:silent": "jest --silent",
    "test:integration": "jest __tests__/integration/ --testPathIgnorePatterns=[]",
    "typecheck": "tsc --noEmit",
    "qualitycheck": "npm-run-all typecheck lint test:silent test:integration"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^7.3.10",
    "@react-navigation/elements": "~2.4.2",
    "@react-navigation/native": "^7.1.6",
    "eslint-config-expo": "~9.2.0",
    "expo": "~53.0.10",
    "expo-constants": "~17.1.6",
    "expo-font": "~13.3.1",
    "expo-linking": "~7.1.5",
    "expo-router": "~5.0.7",
    "expo-splash-screen": "~0.30.9",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.8",
    "expo-updates": "~0.28.14",
    "react": "~19.0.0",
    "react-native": "~0.79.3",
    "react-native-gesture-handler": "~2.24.0",
    "react-native-reanimated": "~3.17.5",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-test-renderer": "~19.0.0"
  },
  "devDependencies": {
    "@babel/core": "~7.27.1",
    "@babel/plugin-transform-export-namespace-from": "~7.27.1",
    "@eslint/js": "^9.28.0",
    "@testing-library/react-native": "~13.2.0",
    "@types/jest": "~29.5.14",
    "@types/react": "~19.0.10",
    "eslint": "~9.27.0",
    "eslint-config-prettier": "~10.1.5",
    "eslint-plugin-prettier": "~5.4.0",
    "jest": "~29.7.0",
    "jest-expo": "~53.0.7",
    "npm-run-all": "~4.1.5",
    "prettier": "~3.5.3",
    "tslib": "~2.8.1",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.0"
  },
  "private": true
}
